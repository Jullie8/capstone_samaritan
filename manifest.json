{
    "manifest_version": 2,
  
    "name": "Getting started with chrome ext to activate on nytimes",
    "description": "This extension allows the user to change the background color of the current page.",
    "version": "1.1",

    "content_scripts": [
        {
          
          "matches": ["http://*.bloomberg.com/*", "http://*.cnn.com/*","http://*.nytimes.com/*","http://*.washingtonpost.com/*", "http://*.yahoo.com/news/*", "http://*.economist.com/*", "http://*.usatoday.com/*", "http://*.huffingtonpost.com/*", "http://*.washingtonpost.com/*", "http://*.wsj.com/*", "http://*.reuters.com/*", "http://*.cnbc.com/*", "http://time.com/*", "http://*.latimes.com/*", "http://*.usnews.com/*", "http://thehill.com/*", "http://*.theatlantic.com/*", "http://*.newsweek.com/*", "http://*.thedailybeast.com/*","http://*.politico.com/*", "http://*.nationalgeographic.com/*", "http://abcnews.go.com/*", "http://*.nbcnews.com/*","http://*.businessinsider.com/*","http://*.vox.com/*","http://slate.com/*", "http://*.sciencedaily.com/*","http://*.theguardian.com/us*", "http://*.bbc.com/new*s","http://*.buzzfeed.com/news?*", "http://*.cbsnews.com/*"],
          "css": ["styles.css"],
          "js": ["popup.js", "watson.js", "app.js", "axios.js"]
        }
      ],

      //Background pages is the parent extension and content scripts cannot modify the dom of background pgs
      "background": {
        "scripts": ["axios.js", "config.js", "background.js", "popup.js"],
        "persistent": false
      },

      "page_action": {
        "default_icon" : "hand.png",
        "default_popup": "popup.html",
        "default_title": "Samaritan"
    },
  


    "permissions": [
      "tabs",
      "activeTab",
      "storage",
      "https://api.data.charitynavigator.org/",
      "https://unpkg.com/axios/dist/axios.min.js/",
      "https://gateway.watsonplatform.net/"

    ]
  }
  

